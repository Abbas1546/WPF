<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:syncfusion="http://schemas.syncfusion.com/wpf">

    <Pen x:Key="ValueCellBorder"
         Brush="#FFD6BE92"
         Thickness="0.25" />

    <Pen x:Key="GroupCellBorder" Thickness="0.5">
        <Pen.Brush>
            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                <GradientStop Offset="0" Color="#FFCB1C23" />
                <GradientStop Offset="1" Color="#FF8E191C" />
            </LinearGradientBrush>
        </Pen.Brush>
    </Pen>

    <LinearGradientBrush x:Key="expanderBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FFF5913F" />
        <GradientStop Offset="1" Color="#FFC14804" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TriangleInner" MappingMode="Absolute" StartPoint="241.030762,459.043945" EndPoint="241.030762,464.498047">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0.000000" Color="#ff404761" />
            <GradientStop Offset="1.000000" Color="#ff617093" />
            <GradientStop Offset="0.539326" Color="#ff617093" />
        </LinearGradientBrush.GradientStops>
        <LinearGradientBrush.Transform>
            <MatrixTransform Matrix="1.000000,0.000000,-0.000000,-1.000000,-197.282227,469.465820" />
        </LinearGradientBrush.Transform>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="headerBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FFCB1C23" />
        <GradientStop Offset="1" Color="#FF8E191C" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="headerHoverBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#FFF33A42" />
        <GradientStop Offset="1" Color="#FF8E191C" />
    </LinearGradientBrush>

