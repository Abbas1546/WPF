    <SampleLayout:SampleLayoutWindow.UserOptionsLayout>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Expander Header="General Options">
                <Grid VerticalAlignment="Top">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="26*" />
                        <RowDefinition Height="26*" />
                        <RowDefinition Height="26*" />
                        <RowDefinition Height="26*"/>

                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="82*"/>
                        <ColumnDefinition Width="62*" />
                        <ColumnDefinition Width="48*"/>
                    </Grid.ColumnDefinitions>
                       <TextBlock TextWrapping="Wrap"   Text="Create Dynamic Path" Grid.Row="3" Margin="5,5,2,5" />
                    <CheckBox Content="" Name="CreateDynamicPath" Margin="8,5,5,5" IsChecked="{Binding IsDynamicCreatePath,Mode=TwoWay,ElementName=shapeControl}" Grid.Column="1" Grid.Row="3" Grid.ColumnSpan="1" />
                </Grid>
            </Expander>
            <Expander Header="Edit Selected Path" Grid.Row="1" IsExpanded="{Binding SelectedMapPath,ElementName=shapeControl,Mode=TwoWay,Converter={StaticResource enableConverter}}" IsEnabled="{Binding SelectedMapPath,ElementName=shapeControl,Mode=TwoWay,Converter={StaticResource enableConverter}}">
                <Grid Name="Edit_Selected_Path" DataContext="{Binding SelectedMapPath,ElementName=shapeControl,Mode=TwoWay}" >
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.55*"/>
                        <ColumnDefinition Width="0.45*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="Path Text" Grid.Row="0" Grid.Column="0" Margin="5" />
                    <TextBox Text="{Binding PathLabel,Mode=TwoWay}" Grid.Column="1" Grid.Row="0" Margin="5" Width="75" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top"  />
                    <TextBlock Text="Path Font Size" Grid.Row="1" Grid.Column="0" Margin="5" />
                    <TextBox Text="{Binding PathLabelFontSize,Mode=TwoWay}" Grid.Column="1" Grid.Row="1" Margin="5"  Width="75"   Height="25" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    <TextBlock Text="Path Font Style" Grid.Row="2" Grid.Column="0" Margin="5" />
                    <ComboBox Name="PathLabelFontStyle" SelectedIndex="{Binding PathLabelFontStyle,Mode=TwoWay, Converter={StaticResource SelectedIndexToPathLabelFontStyle}}" Grid.Column="1" Grid.Row="2" Margin="5"  Width="75" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <ComboBoxItem>Normal</ComboBoxItem>
                        <ComboBoxItem>Italic</ComboBoxItem>
                    </ComboBox>
                    <TextBlock Text="Path Label Position" Grid.Row="3" Grid.Column="0" Margin="5"  />
                    <ComboBox Name="PathLabelPositioncombo" SelectedIndex="{Binding PathLabelPosition,Mode=TwoWay,Converter={StaticResource selectedValueToLabelPoint}}" Grid.Column="1" Grid.Row="3" Margin="5"  Width="75" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <ComboBoxItem>OnMiddlePoint</ComboBoxItem>
                        <ComboBoxItem>OnPoint</ComboBoxItem>
                    </ComboBox>
                    <TextBlock Text="Path Label Foreground" TextWrapping="Wrap"   Grid.Row="4" Grid.Column="0" Margin="5"  />
                    <ComboBox Name="PathLabelForeground" SelectedIndex="{Binding PathLabelForeground,Mode=TwoWay,Converter={StaticResource brushconverter}}" Grid.Column="1" Grid.Row="4" Margin="5"  Width="75" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <ComboBoxItem>White</ComboBoxItem>
                        <ComboBoxItem>Black</ComboBoxItem>
                        <ComboBoxItem>Cyan</ComboBoxItem>
                        <ComboBoxItem>Blue</ComboBoxItem>
                        <ComboBoxItem>Red</ComboBoxItem>
                    </ComboBox>
                    <TextBlock Text="Path Color" Grid.Row="5" Grid.Column="0" Margin="5"  />
                    <ComboBox Name="PathColor" SelectedIndex="{Binding PathColor,Mode=TwoWay,Converter={StaticResource brushconverter}}" Grid.Column="1" Grid.Row="5" Margin="5"  Width="75" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <ComboBoxItem>White</ComboBoxItem>
                        <ComboBoxItem>Black</ComboBoxItem>
                        <ComboBoxItem>Cyan</ComboBoxItem>
                        <ComboBoxItem>Blue</ComboBoxItem>
                        <ComboBoxItem>Red</ComboBoxItem>
                    </ComboBox>
                </Grid>
            </Expander>
        </Grid>
    </SampleLayout:SampleLayoutWindow.UserOptionsLayout>
  </SampleLayout:SampleLayoutWindow>
