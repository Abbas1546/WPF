        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf" WindowState="Maximized"
        xmlns:this="clr-namespace:RichTextBoxAdvSample" Icon="App.ico"
        Title="{Binding Path=DocumentTitle, ElementName=rte}" >
    
        <syncfusion:RibbonWindow.Resources>
            <this:ZoomFactorInPercentage x:Key="FactorInPercentage"/>
        </syncfusion:RibbonWindow.Resources>

    <syncfusion:RibbonWindow.StatusBar>
        <syncfusion:RibbonStatusBar Height="25">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" VerticalAlignment="Center">
                    <TextBlock Margin="10,0,0,5" FontSize="12">
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="Section :"/>
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="{Binding Path=Document.Sections.Count, Mode=OneWay}"/>
                    </TextBlock>
                    <TextBlock Margin="10,0,0,5" FontSize="12">
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="Page :" />
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="{Binding Path=Viewer.CurrentPageNumber}"/>
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="Of"/>
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" x:Name="pagecount" RunText="{Binding PageCount}"/>
                    </TextBlock>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="1">
                    <syncfusion:RibbonButton SizeForm="ExtraSmall" x:Name="print" SmallIcon="/Images/PrintLayout.png" Command="syncfusion:RichTextBoxAdv.ChangeLayout" CommandTarget="{Binding ElementName=rte}" CommandParameter="Pages"/>
                    <syncfusion:RibbonButton SizeForm="ExtraSmall" x:Name="web"  SmallIcon="/Images/WebLayout.png" Command="syncfusion:RichTextBoxAdv.ChangeLayout" CommandTarget="{Binding ElementName=rte}" CommandParameter="Continuous"/>
                    <TextBlock FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="{Binding Path=ZoomFactor, Converter={StaticResource FactorInPercentage}}"/>
								<this:RunAdv Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" RunText="%"/>
                    </TextBlock>
                    <Slider x:Name="rangeslider" Foreground="{Binding Path=Foreground,
                                                Mode=OneWay,
                                                RelativeSource={RelativeSource FindAncestor,
                                                                               AncestorType={x:Type syncfusion:RibbonStatusBar}}}" Value="0.25" TickFrequency="0.1" Width="150"
											    TickPlacement="None" Minimum="0" Maximum="1" />
                </StackPanel>
            </Grid>
        </syncfusion:RibbonStatusBar>
    </syncfusion:RibbonWindow.StatusBar>
    
