          x:Name="PART_ReseizerThumb1"  StrokeDashCap="Flat" StrokeStartLineCap="Round" StrokeThickness="1" Margin="-5 0 0 0" Stroke="OrangeRed" Fill="Orange" />
                        </Grid>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


