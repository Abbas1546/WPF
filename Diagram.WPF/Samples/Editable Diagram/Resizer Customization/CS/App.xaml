                        <Grid >
                            <Border x:Name="PART_ResizerBorder" Width="{TemplateBinding Width}"  HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
          VerticalAlignment="{TemplateBinding VerticalAlignment}" Cursor="{TemplateBinding Cursor}" Margin="{TemplateBinding Margin}"
           CornerRadius="7" 
